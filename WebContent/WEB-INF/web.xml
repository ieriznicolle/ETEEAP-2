<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">


  <display-name>ETEEAP_AS</display-name>

 <!-- DEFAULT --> 
	<servlet>
		<servlet-name>FrontNav</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPHP</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>FrontNav</servlet-name>
		<url-pattern>/eteeap/*</url-pattern>
	</servlet-mapping> 

<!-- ACCOUNT --> 
	<servlet>
		<servlet-name>Account</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPLI</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Account</servlet-name>
		<url-pattern>/login/*</url-pattern>
	</servlet-mapping>

<!-- ADMIN ACCOUNT --> 
	<servlet>
		<servlet-name>AdminAccountNav</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPALI</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AdminAccountNav</servlet-name>
		<url-pattern>/account/*</url-pattern>
	</servlet-mapping>


<!-- SETTINGS --> 
	<servlet>
		<servlet-name>Settings</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPSettings</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Settings</servlet-name>
		<url-pattern>/settings/*</url-pattern>
	</servlet-mapping>
	
<!-- ACCOUNT SETTINGS --> 
	<servlet>
		<servlet-name>Account Settings</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPSettings</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Account Settings</servlet-name>
		<url-pattern>/accountsettings/*</url-pattern>
	</servlet-mapping>
	
<!-- STUDENTS --> 
	<servlet>
		<servlet-name>Students</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPStudents</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Students</servlet-name>
		<url-pattern>/students/*</url-pattern>
	</servlet-mapping>
	
<!-- STUDENT DETAILS --> 
	<servlet>
		<servlet-name>Student Details</servlet-name>
		<servlet-class>eteeap.controller.ETEEAPStudentDetails</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Student Details</servlet-name>
		<url-pattern>/studentdetails/*</url-pattern>
	</servlet-mapping>
	
<!--  --> 
	<servlet>
		<servlet-name>checklist</servlet-name>
		<servlet-class>eteeap.controller.checklist</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>checklist</servlet-name>
		<url-pattern>/checklist</url-pattern>
	</servlet-mapping>
 
</web-app>